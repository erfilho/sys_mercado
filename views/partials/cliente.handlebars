<div
  class="d-flex flex-row rounded border border-1 justify-content-around align-items-center align-content-center"
>
  <p class="col-1 text-center align-content-center align-items-center my-0">
    {{this.id}}
  </p>
  <p class="col-5 my-0"> {{this.name}} </p>
  <p class="col-3 my-0"> {{this.cpf}} </p>
  <p class="col-3 my-0"> {{this.phone}} </p>
  <button class="col-2 my-0 btn btn-danger" onclick="verCad({{this.id}})">
    Ver cadastro
  </button>
</div>